{
  "name": "neysla",
  "version": "3.8.1",
  "description": "Promise based HTTP RESTful API resources modeler for the browser",
  "main": "build/module/neysla.min.js",
  "types": "build/module/neysla.d.ts",
  "scripts": {
    "test": "karma start",
    "test-travis": "karma start --single-run",
    "serve": "webpack-dev-server --open",
    "build": "npm run build-module && npm run build-cdn",
    "set-types": "cp -r ./app/neysla.d.ts ./build/module/neysla.d.ts",
    "build-module": "npm run set-types && uglifyjs app/neysla.js --compress --mangle reserved=['Neysla'] --output build/module/neysla.min.js --wrap Neysla",
    "build-cdn": "browserify -e app/neysla.js -s Neysla -t [ babelify ] | uglifyjs --compress --mangle --output build/cdn/neysla.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onca-vega/Neysla.git"
  },
  "keywords": [
    "javascript",
    "js",
    "http",
    "request",
    "ajax",
    "xhr",
    "promise",
    "modeler",
    "client",
    "neysla"
  ],
  "author": "Marcos Jesús Chávez Vega",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/onca-vega/Neysla/issues"
  },
  "homepage": "https://github.com/onca-vega/Neysla#readme",
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@babel/preset-env": "^7.15.4",
    "babel-loader": "^8.2.2",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "core-js": "^3.17.2",
    "html-webpack-plugin": "^5.3.2",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine": "^3.6.1",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-coveralls": "^2.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-webpack": "^5.0.0",
    "sinon": "^11.1.2",
    "uglify-js": "^3.17.4",
    "webpack": "^5.90.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.1"
  }
}
